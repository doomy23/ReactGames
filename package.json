{
  "name": "reactgames",
  "version": "1.0.0",
  "description": "ReactJS games with websockets and redux architechture",
  "private": true,
  "scripts": {
    "build": "webpack --config server/webpack.config.js  --color -p --progress --hide-modules",
    "watch": "webpack --config server/webpack.config.js --watch",
    "server": "node server --color -p --progress --hide-modules",
    "test": "mocha test --recursive",
    "cover": "nyc mocha test --recursive",
    "eslint": "eslint src test --fix",
    "eslint_prepush": "eslint src test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doomy23/ReactGames.git"
  },
  "keywords": [
    "reactjs",
    "game",
    "websockets"
  ],
  "author": "Dominic Roberge",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/doomy23/ReactGames/issues"
  },
  "homepage": "https://github.com/doomy23/ReactGames#readme",
  "configs": {
    "reactgames": {
      "db": {
        "database": null,
        "username": null,
        "password": null,
        "options": {
          "dialect": "sqlite",
          "storage": ":memory:"
        }
      },
      "cookies": {
        "secret": "VZQVOO5zyEMI3Des",
        "maxAge": 8640000
      },
      "session": {
        "expires_in": 15,
        "expires_update_each": 5
      }
    }
  },
